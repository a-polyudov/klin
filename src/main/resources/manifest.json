{
  "name": "klin",
  "description": "Tiny tool for easy opening an JIRA issue",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+Y",
        "linux": "Ctrl+Shift+Y"
      }
    }
  }
}